package tool;

import java.io.File;
import java.io.StringReader;

import javax.swing.text.Document;

import org.dom4j.io.SAXReader;

public class CreateFile {
	public static void createFile(String interfacename, String input){
		File file=new File("C:\\temp\\I"+interfacename +".wsdl");
		formatToXML(input, file);
	}

	private static void formatToXML(String str, File file) {
		// TODO Auto-generated method stub
		SAXReader reader=new SAXReader();
		StringReader stringReader=new StringReader(str);
		Document document;
		document=reader.read(stringReader);
		stringReader.close();
	}
}
